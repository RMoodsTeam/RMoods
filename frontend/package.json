{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "build-docs": "typedoc --plugin typedoc-plugin-rename-defaults --entryPointStrategy expand app/",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cypress": "^13.13.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5"
  }
}