{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "build-docs": "typedoc --plugin typedoc-plugin-rename-defaults --entryPointStrategy expand src/",
    "e2e": "start-server-and-test dev http://localhost:8000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:8000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.9.3",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@react-oauth/google": "^0.12.1",
    "framer-motion": "^11.9.0",
    "jotai": "^2.10.0",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/cypress": "^1.1.3",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cypress": "^13.15.0",
    "eslint": "^9.9.0",
